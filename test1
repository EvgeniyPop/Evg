#First number to start with
number_in_row = "1"
#Show first n numbers on the row
for i in range(n):
	new_number = "" #Themp for the new number
	digit_counter = 0 #Counter for digit changing
	for number_index in range(1, len(number_in_row)):
		digit_counter += 1
		if number_in_row[number_index] != number_in_row[number_index - 1]:
			new_number += "%d%s" % (digit_counter, number_in_row[number_index - 1])
			digit_counter = 0
	new_number += "%d%s" % (digit_counter + 1, number_in_row[-1] #Last number case
	print(new_number)
	number_in_row = new_number
			
